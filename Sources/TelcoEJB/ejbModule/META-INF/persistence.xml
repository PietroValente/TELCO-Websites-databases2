<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="TelcoEJB" transaction-type="RESOURCE_LOCAL">
		<jta-data-source>telco_db</jta-data-source>
		<class>model.Alert</class>
		<class>model.Optionalproduct</class>
		<class>model.Optionalproductsalesreport</class>
		<class>model.Pack</class>
		<class>model.Packageservice</class>
		<class>model.PackageservicePK</class>
		<class>model.Packsalesreport</class>
		<class>model.Purchase</class>
		<class>model.Purchasesalesreport</class>
		<class>model.Service</class>
		<class>model.Session</class>
		<class>model.User</class>
		<class>model.Usersalesreport</class>
		<class>model.Validityperiod</class>
		<class>model.ValidityperiodPK</class>
		<class>model.Validityperiodsalesreport</class>
		<class>model.ValidityperiodsalesreportPK</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="javax.persistence.lock.timeout" value="1000"/>
			<property name="RuntimeUnenhancedClasses" value="supported"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/telco"/>
			<property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.password" value="mysql"/>
		</properties>
	</persistence-unit>
</persistence>
